#Editor Config documentation: http://editorconfig.org

# Top-most EditorConfig file
root=true

# For all files
[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# For .md files
[*.md]
trim_trailing_whitespace = false
indent_style = tab
